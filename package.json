{
  "name": "fire-util-flutter",
  "displayName": "Fire Util Flutter",
  "description": "utility for firebase and flutter",
  "icon": "assets/logo.png",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahnaf16/fire-util-flutter"
  },
  "author": {
    "name": "Ahnaf"
  },
  "publisher": "Ahnaf-MARJ",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "flutter",
    "dart"
  ],
  "activationEvents": [
    "onLanguage:dart",
    "onCommand:fire-util-flutter.helloWorld"
  ],
  "main": "src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fire-util-flutter.generateStaticFieldNames",
        "title": "Generate Static Fields Names"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "fire-util-flutter.generateStaticFieldNames",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}